{
    "name": "botman/studio",
    "description": "BotMan Chatbot framework.",
    "keywords": [
        "botman",
        "chatbot",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "babenkoivan/scout-elasticsearch-driver": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.4",
        "botman/botman": "~2.0",
        "botman/driver-slack": "^2.1",
        "botman/driver-web": "~1.0",
        "botman/installer": "^1.0",
        "botman/studio-addons": "~1.2.1",
        "botman/tinker": "~1.0",
        "davejamesmiller/laravel-breadcrumbs": "~5.0.0",
        "doctrine/dbal": "^2.7",
        "facebook/webdriver": "^1.5",
        "fideloper/proxy": "~4.0",
        "fzaninotto/faker": "^1.4",
        "intervention/image": "^2.4",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.6.0",
        "mpociot/slack-client": "^1.0",
        "predis/predis": "^1.1",
        "react/event-loop": "^0.4.3",
        "rtconner/laravel-tagging": "~2.2"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^4.0",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "7.1.4",
        "nunomaduro/collision": "^2.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
